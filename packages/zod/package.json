  },
  "homepage": "https://basketry.io/docs/components/@basketry/zod",
  "funding": "https://github.com/sponsors/basketry",
  "dependencies": {
    "@basketry/typescript": "^0.2.3",
    "basketry": "^0.2.1",
    "case": "^1.6.3",
    "pluralize": "^8.0.0",
    "prettier": "^3.6.2"
  }
