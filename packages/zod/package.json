{
  "name": "@basketry/zod",
  "version": "0.2.9",
  "description": "Basketry generator for generating zod schemas",
  "main": "./lib/index.js",
  "bin": {
    "basketry-zod": "./lib/rpc.js"
  },
  "scripts": {
    "prebuild": "run-s -s clean",
    "build": "tsc",
    "postbuild": "chmod +x ./lib/rpc.js",
