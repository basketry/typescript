{
  "name": "@basketry/typescript",
  "version": "0.2.0-alpha.1",
  "description": "Basketry generator for generating Typescript interfaces",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "clean": "run-s -s clean:*",
    "create-snapshot": "ts-node ./src/snapshot/create-snapshot.ts",
    "lint": "run-s -s lint:*",
    "fix": "run-s -s fix:*",
    "clean:output": "rimraf lib",
    "start": "node ./lib/index.js",
    "prebuild": "run-s -s clean lint",
    "build": "tsc",
    "lint:eslint": "eslint",
    "fix:eslint": "eslint --fix",
    "lint:prettier": "prettier -c .",
    "fix:prettier": "prettier -w .",
    "clean:coverage": "rimraf coverage",
    "pretest": "run-s -s clean",
    "prepack": "run-s -s build"
  },
  "keywords": [],
  "author": "Steve Konves",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basketry/typescript.git"
  },
  "bugs": {
    "url": "https://github.com/basketry/typescript/issues"
  },
  "homepage": "https://github.com/basketry/typescript#readme",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.4.0",
    "typescript": "<5.6.0"
  },
  "dependencies": {
    "basketry": "^0.2.0-alpha.6",
    "case": "^1.6.3",
    "prettier": "^3.3.3"
  }
}
